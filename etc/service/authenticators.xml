<authenticators-list xmlns="http://software.in2p3.fr/lavoisier/application.xsd">
    <!-- used by library "gui-admin" -->
    <authenticators name="admin">
        <authenticator type="IPAddressAuthenticator">
            <parameter name="addresses">
                <entry>127.0.0.1</entry>
                <entry>0:0:0:0:0:0:0:1</entry>
                <entry>134.158.231.28</entry>
            </parameter>
        </authenticator>
    </authenticators>

    <authenticators name="aai">
        <authenticator type="OAuth2Authenticator">
            <!-- required: The URL of the OAuth2 Authorization URL (String) -->
            <parameter name="authz_location">https://aai-dev.egi.eu/oidc/authorize</parameter>
            <!-- required: The URL of the OAuth2 Token URL (String) -->
            <parameter name="token_location">https://aai-dev.egi.eu/oidc/token</parameter>
            <!-- required: The set of permissions requested (String) -->
            <parameter name="scope">openid profile email eduperson_entitlement eduperson_scoped_affiliation</parameter>
            <!-- required: The Client ID (or App ID) (String) -->
            <parameter name="client_id" eval="property('egi.client_id')"/>
            <!-- required: The Client secret (or App secret) (String) -->
            <parameter name="client_secret" eval="property('egi.client_secret')"/>
        </authenticator>

        <authenticator type="IPAddressAuthenticator">
            <parameter name="addresses">
                <entry>127.0.0.1</entry>
                <entry>0:0:0:0:0:0:0:1</entry>
                <entry>134.158.231.28</entry>
            </parameter>
        </authenticator>

    </authenticators>

